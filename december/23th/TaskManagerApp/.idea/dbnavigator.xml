      <console name="TablesManager" type="STANDARD" schema="information_schema" session="Main"><![CDATA[create database task_manager_app_db;
drop database task_manager_app_db;

create table "user" (
    id bigserial,
    login character varying(30) not null,
    email character varying(30) not null,
    password character varying(30) not null,
    unique (login),
    unique (email),
    primary key (id)
);

create table task_category (
    id bigserial,
    "name" character varying(30) not null,
    unique ("name"),
    primary key (id)
);

create table task (
    id bigserial,
    "name" character varying(30) not null,
    task_category_id bigint not null DEFAULT 1,
    primary key (id),
    foreign key (task_category_id) references task_category (id) ON DELETE SET DEFAULT
);

create table project (
    id bigserial,
    "name" character varying(30) not null,
    key character varying(30) not null,
    unique (key),
    primary key (id)
);

create table task_details (
    id bigserial,
    project_id bigint not null,
    task_id bigint,
    author bigint not null,
    executor bigint,
    unique (project_id, task_id),
    primary key (id),
    foreign key (project_id) references project (id) ON DELETE CASCADE,
    foreign key (task_id) references task (id) ON DELETE CASCADE,
    foreign key (author) references "user" (id) ON DELETE CASCADE,
    foreign key (executor) references "user" (id) ON DELETE SET NULL
);]]></console>
      <console name="TaskCategoryTable" type="STANDARD" schema="public" session="Main"><![CDATA[select * from task_category;

SELECT * FROM task_category WHERE name = 'To do' OR name = 'In progress' OR name = 'Done';

CREATE OR REPLACE FUNCTION save_task_category(id_project BIGINT, task_category_name CHARACTER VARYING) RETURNS BOOLEAN AS $$
    DECLARE
        tc_id BIGINT;
    BEGIN 
        INSERT INTO task_category ("name") VALUES ($2) RETURNING "id" INTO tc_id;
        INSERT INTO task_details (project_id, author) VALUES (pr_id, $3);
    END;
$$ LANGUAGE 'plpgsql';]]></console>
      <console name="TaskDetailsTable" type="STANDARD" schema="public" session="Main"><![CDATA[select * from task_details;

select td.id as td_id, p."id" as p_id, p."name" as p_name, p."name" as p_key,
       t.id as t_id, t."name" as t_name, tca.id as tc_id, tca."name" as tc_name,
       author.id as author_id, author.login as author_login, executor.id as executor_id, 
       executor.login as executor_login
from task_details td 
inner join project p on td.project_id = p."id"
left join task t on td.task_id = t."id"
inner join task_category tca on t.task_category_id = tca."id"
inner join "user" author on td.author = author."id"
left join "user" executor on td.executor = executor.id
where td.author = 3;

UPDATE task_details SET author = ? WHERE author = ?;


CREATE OR REPLACE FUNCTION transfer_project_to_user(project BIGINT, new_author BIGINT) RETURNS BOOLEAN AS $$
    BEGIN 
        UPDATE task_details SET author = $2 WHERE project_id = $1;
        RETURN FOUND;
    END;
$$ LANGUAGE 'plpgsql';

select transfer_project_to_user(1, 4);]]></console>
      <console name="TaskTable" type="STANDARD" schema="public" session="Main"><![CDATA[select * from task;

select t.id as t_id,
       t."name" as t_name,
       tc.id as tc_id,
       tc."name" as tc_name,
       td."id" as td_id,
       p."id" as p_id,
       p."id" as p_id,
       p."name" as p_name,
       p."key" as p_key,
       au."id" as au_id,
       au."login" as au_login,
       ex.id as ex_id,
       ex.login as ex_login
from task t
inner join task_category tc on t.task_category_id = tc."id"
inner join task_details td on t.id = td.task_id
inner join project p on td.project_id = p."id"
inner join "user" au on td.author = au."id"
left join "user" ex on td.executor = ex.id
where p."id" = 4;



CREATE OR REPLACE FUNCTION create_task(task_name CHARACTER VARYING, id_task_category BIGINT, id_project BIGINT, author_id BIGINT) RETURNS BOOLEAN AS $$
    DECLARE
        id_task BIGINT;
        selected_task_details task_details%ROWTYPE;   
    BEGIN 
        INSERT INTO task ("name", task_category_id) VALUES ($1, $2) RETURNING "id" INTO id_task;
        SELECT "id" FROM task_details INTO selected_task_details WHERE project_id = $3 AND task_id IS NULL LIMIT 1;
        IF selected_task_details IS NULL THEN
            INSERT INTO task_details (project_id, task_id, author) VALUES ($3, id_task, $4);
        ELSE
            UPDATE task_details SET task_id = id_task WHERE "id" = selected_task_details."id";
        END IF;
        RETURN FOUND;
    END;
$$ LANGUAGE 'plpgsql';]]></console>
      <console name="UserTable" type="STANDARD" schema="public" session="Main"><![CDATA[select * from "user";

CREATE OR REPLACE FUNCTION delete_user(id_user BIGINT) RETURNS BOOLEAN AS $$
    DECLARE
        _rec RECORD;
    BEGIN 
    
        FOR _rec IN 
            SELECT * FROM task_details WHERE author = $1
        LOOP 
            DELETE FROM project WHERE "id" = _rec.project_id;
            DELETE FROM task WHERE "id" = _rec.task_id;
            DELETE FROM task_details WHERE "id" = _rec."id";
        END LOOP;
        
        DELETE FROM "user" WHERE "id" = $1;
        
        RETURN FOUND;
    END;
$$ LANGUAGE 'plpgsql';]]></console>
